{
  "name": "kineticslider",
  "version": "1.0.0",
  "description": "A high-performance, customizable slider component with kinetic scrolling and advanced effects",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src --ext .ts,.tsx",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css,scss,md}\"",
    "prepublishOnly": "npm test && npm run lint && npm run build",
    "docs": "typedoc --out docs src",
    "release": "semantic-release"
  },
  "keywords": [
    "react",
    "slider",
    "kinetic",
    "scroll",
    "effects",
    "filters",
    "performance",
    "typescript"
  ],
  "author": "Zach Atkinson",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zachatkinson/kineticslider.git"
  },
  "bugs": {
    "url": "https://github.com/zachatkinson/kineticslider/issues"
  },
  "homepage": "https://github.com/zachatkinson/kineticslider#readme",
  "peerDependencies": {
    "react": "^18.0.0 || ^19.0.0",
    "react-dom": "^18.0.0 || ^19.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.26.10",
    "@babel/preset-env": "^7.26.9",
    "@babel/preset-react": "^7.26.3",
    "@babel/preset-typescript": "^7.27.0",
    "@rollup/plugin-commonjs": "^28.0.3",
    "@rollup/plugin-node-resolve": "^16.0.1",
    "@rollup/plugin-replace": "^6.0.2",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^12.1.2",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.2.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^29.5.14",
    "@types/react": "^19.0.12",
    "@types/react-dom": "^19.0.4",
    "@typescript-eslint/eslint-plugin": "^8.28.0",
    "@typescript-eslint/parser": "^8.28.0",
    "@eslint/js": "^9.23.0",
    "babel-jest": "^29.7.0",
    "babel-plugin-module-resolver": "^5.0.2",
    "eslint": "^9.23.0",
    "eslint-config-prettier": "^10.1.1",
    "eslint-plugin-prettier": "^5.2.4",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-hooks": "^5.2.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "lru-cache": "^11.1.0",
    "prettier": "^3.5.3",
    "read-package-up": "^11.0.0",
    "rimraf": "^6.0.1",
    "rollup": "^4.37.0",
    "rollup-plugin-dts": "^6.2.1",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-postcss": "^4.0.2",
    "sass": "^1.86.0",
    "semantic-release": "^24.2.3",
    "ts-jest": "^29.3.0",
    "typedoc": "^0.28.1",
    "typescript": "^5.8.2"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  }
}
