{
  "name": "kineticslider",
  "version": "0.1.0",
  "private": false,
  "description": "A modern, accessible, and performant slider component",
  "author": "Zach",
  "license": "MIT",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "vite build",
    "dev": "vite",
    "preview": "vite preview",
    "start": "pnpm run dev",
    "clean": "rimraf dist coverage .jest-cache",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --reporters=default --reporters=jest-sonar-reporter",
    "test:e2e": "jest --config jest.config.e2e.js",
    "lint": "eslint . --ext .ts,.tsx",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,json,md}\"",
    "typecheck": "tsc --noEmit",
    "validate": "pnpm run typecheck && pnpm run lint && pnpm run test",
    "prepublishOnly": "pnpm run clean && pnpm run validate && pnpm run build",
    "postpublish": "git push --follow-tags",
    "analyze": "vite build --mode analyze",
    "size": "size-limit",
    "audit": "pnpm audit fix",
    "docs": "typedoc src/index.ts",
    "prepare": "husky && husky install"
  },
  "keywords": [
    "react",
    "slider",
    "typescript",
    "accessible",
    "component"
  ],
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zach/kineticslider.git"
  },
  "bugs": {
    "url": "https://github.com/zach/kineticslider/issues"
  },
  "homepage": "https://github.com/zach/kineticslider#readme",
  "contributing": {
    "type": "individual",
    "url": "https://github.com/zach/kineticslider/blob/main/CONTRIBUTING.md"
  },
  "overrides": {
    "@babel/traverse": "^7.23.2"
  },
  "peerDependencies": {
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.26.10",
    "@babel/plugin-transform-class-properties": "^7.25.9",
    "@babel/plugin-transform-private-methods": "^7.25.9",
    "@babel/plugin-transform-runtime": "^7.26.10",
    "@babel/preset-env": "^7.26.9",
    "@babel/preset-react": "^7.26.3",
    "@babel/preset-typescript": "^7.27.0",
    "@babel/runtime": "^7.27.0",
    "@commitlint/cli": "^19.8.0",
    "@commitlint/config-conventional": "^19.8.0",
    "@eslint/js": "9.23.0",
    "@size-limit/preset-small-lib": "^11.2.0",
    "@swc/jest": "0.2.37",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.2.0",
    "@testing-library/user-event": "^14.6.1",
    "@trivago/prettier-plugin-sort-imports": "^5.2.2",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.13.14",
    "@types/react": "^19.0.12",
    "@types/react-dom": "^19.0.4",
    "@typescript-eslint/eslint-plugin": "^8.28.0",
    "@typescript-eslint/parser": "^8.28.0",
    "@vitejs/plugin-react": "^4.3.4",
    "babel-jest": "^29.7.0",
    "core-js": "^3.41.0",
    "eslint": "9.23.0",
    "eslint-config-prettier": "^10.1.1",
    "eslint-plugin-jsdoc": "^50.6.9",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-hooks": "^5.2.0",
    "husky": "^8.0.0",
    "identity-obj-proxy": "3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-sonar-reporter": "^2.0.0",
    "jest-watch-typeahead": "^2.2.2",
    "lint-staged": "^15.5.0",
    "prettier": "^3.5.3",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "rimraf": "^6.0.1",
    "size-limit": "^11.2.0",
    "typedoc": "^0.28.1",
    "typescript": "^5.8.2",
    "vite": "^6.2.3",
    "vite-plugin-compression": "^0.5.1",
    "vite-tsconfig-paths": "^5.1.4"
  },
  "dependencies": {
    "gsap": "^3.12.7",
    "pixi.js": "^8.9.0"
  },
  "publishConfig": {
    "access": "restricted",
    "registry": "https://registry.npmjs.org/",
    "tag": "latest"
  },
  "size-limit": [
    {
      "path": "dist/index.js",
      "limit": "10 kb"
    },
    {
      "path": "dist/index.esm.js",
      "limit": "10 kb"
    }
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "typesVersions": {
    "*": {
      "*": [
        "./dist/index.d.ts"
      ]
    }
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not op_mini all"
  ],
  "funding": {
    "type": "github",
    "url": "https://github.com/sponsors/zach"
  },
  "resolutions": {
    "@babel/traverse": "^7.23.2"
  }
}
