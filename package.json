{
  "name": "kineticslider",
  "version": "0.1.0",
  "private": false,
  "description": "A modern, accessible, and performant slider component",
  "author": "Zach",
  "license": "MIT",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "vite build",
    "dev": "vite",
    "preview": "vite preview",
    "start": "pnpm run dev",
    "clean": "rimraf dist coverage .jest-cache",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --reporters=default --reporters=jest-sonar-reporter",
    "test:e2e": "jest --config jest.config.e2e.js",
    "lint": "eslint . --ext .ts,.tsx",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,json,md}\"",
    "typecheck": "tsc --noEmit",
    "validate": "pnpm run typecheck && pnpm run lint && pnpm run test",
    "prepare": "husky install",
    "prepublishOnly": "pnpm run clean && pnpm run validate && pnpm run build",
    "postpublish": "git push --follow-tags",
    "analyze": "vite build --mode analyze",
    "size": "size-limit",
    "audit": "pnpm audit fix",
    "docs": "typedoc src/index.ts"
  },
  "keywords": [
    "react",
    "slider",
    "typescript",
    "accessible",
    "component"
  ],
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zach/kineticslider.git"
  },
  "bugs": {
    "url": "https://github.com/zach/kineticslider/issues"
  },
  "homepage": "https://github.com/zach/kineticslider#readme",
  "contributing": {
    "type": "individual",
    "url": "https://github.com/zach/kineticslider/blob/main/CONTRIBUTING.md"
  },
  "overrides": {
    "@babel/traverse": "^7.23.2"
  },
  "peerDependencies": {
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.24.0",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-private-methods": "^7.18.6",
    "@babel/plugin-transform-runtime": "^7.24.0",
    "@babel/preset-env": "^7.24.0",
    "@babel/preset-react": "^7.23.3",
    "@babel/preset-typescript": "^7.23.3",
    "@babel/runtime": "^7.24.0",
    "@commitlint/cli": "^18.6.1",
    "@commitlint/config-conventional": "^18.6.2",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^14.2.1",
    "@testing-library/user-event": "^14.5.2",
    "@trivago/prettier-plugin-sort-imports": "^5.2.2",
    "@types/jest": "^29.5.12",
    "@types/prettier": "^3.0.0",
    "@types/react": "^18.2.64",
    "@types/react-dom": "^18.2.21",
    "@typescript-eslint/eslint-plugin": "^7.1.0",
    "@typescript-eslint/parser": "^7.1.0",
    "babel-jest": "^29.7.0",
    "core-js": "^3.36.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-jsdoc": "^48.2.0",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^9.0.11",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-sonar-reporter": "^2.0.0",
    "jest-watch-typeahead": "^2.2.2",
    "lint-staged": "^15.2.2",
    "prettier": "^3.2.5",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "rimraf": "^5.0.5",
    "@size-limit/preset-small-lib": "^11.0.0",
    "size-limit": "^11.0.0",
    "typedoc": "^0.25.0",
    "@types/node": "^20.0.0",
    "typescript": "^5.3.3",
    "vite": "^5.0.0",
    "@vitejs/plugin-react": "^4.2.0",
    "vite-plugin-compression": "^0.5.1",
    "vite-tsconfig-paths": "^4.3.1",
    "@types/vite": "^5.0.0",
    "@babel/plugin-transform-class-properties": "^7.23.3",
    "@babel/plugin-transform-private-methods": "^7.23.3"
  },
  "dependencies": {
    "gsap": "^3.12.5",
    "pixi.js": "^7.4.0"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,css,scss,md}": [
      "prettier --write"
    ]
  },
  "workspaces": {
    "packages": [
      "packages/*"
    ],
    "nohoist": [
      "**/react",
      "**/react-dom"
    ]
  },
  "publishConfig": {
    "access": "restricted",
    "registry": "https://registry.npmjs.org/",
    "tag": "latest"
  },
  "size-limit": [
    {
      "path": "dist/index.js",
      "limit": "10 kb"
    },
    {
      "path": "dist/index.esm.js",
      "limit": "10 kb"
    }
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "typesVersions": {
    "*": {
      "*": [
        "./dist/index.d.ts"
      ]
    }
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not op_mini all"
  ],
  "funding": {
    "type": "github",
    "url": "https://github.com/sponsors/zach"
  },
  "resolutions": {
    "@babel/traverse": "^7.23.2"
  }
}
